{
    "manifest_version": 3,
    "name": "Bookmark Cleaner",
    "version": "0.0.1",
    "description": "A Chrome extension to clean up your bookmarks",
    "permissions": [
        "bookmarks"
    ],
    "action": {
        "default_popup": "popup/popup.html",
        "default_icon": {
            "16": "icons/icon-16.png",
            "48": "icons/icon-48.png",
            "128": "icons/icon-128.png"
        }
    },
    "icons": {
        "16": "icons/icon-16.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
    }
}